{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "indent": "off",
        "object-curly-spacing":"off",
        "no-unused-vars":"warn",
        "max-len":"off"

    }
}
